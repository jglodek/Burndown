
<table border>
<tr>
	<td>Title</td>
	<td>Description</td>
	<td>Created at</td>
	<td>Finished at</td>
	<td>Members</td>
	<td>Your role</td>
</tr>
<% @all_projects.each do |p| %>
<tr>
	<td><%= link_to p.title, p %></td>
	<td><%= p.description %></td>
	<td><%= p.created_at %></td>
	<td><%= p.finished_at ? p.finished_at : "Unfinished" %></td>
	<td><%= p.project_members_count %></td>
	<td><%= project_role_to_string :role =>0%></td>
</tr>
<% end %>
</table>

<%= link_to 'Create new project', new_project_path() %>
